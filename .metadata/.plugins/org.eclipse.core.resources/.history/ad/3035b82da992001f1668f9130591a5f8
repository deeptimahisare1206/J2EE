package manytomany;


import jakarta.persistence.Entity;
import jakarta.persistence.Id;
import jakarta.persistence.ManyToMany;


@Entity
public class Answer2 {
	
	@Id
	int aid;
	String answer;
	@ManyToMany
	QuestionM que;
	public int getAid() {
		return aid;
	}
	public void setAid(int aid) {
		this.aid = aid;
	}
	public String getAnswer() {
		return answer;
	}
	public void setAnswer(String answer) {
		this.answer = answer;
	}
	public QuestionM getQue() {
		return que;
	}
	public void setQue(QuestionM que) {
		this.que = que;
	}
	

}
